@charset "utf-8";
/* CSS Document */

/*新房详情页*/
.main{}
.pdb50{ padding-bottom:50px !important}
.xfxq{ padding: 15px 15px;}
.xfxq p{ font-size:16px; color:#000000; line-height:20px;}
.xfxq .byname span{ display:inline-block; font-size:12px; color:#888888; line-height:20px; margin-top:11px;}
.xfxq .stag{ margin-top:12px;}

.xfbox{ position:relative;}
.lp-price{ padding:15px 15px 0;}
.lp-price .red-df{ font-size:15px; color:#df3031; line-height:20px;}   
.lp-taps{ padding:10px 15px 0;}
.lp-taps span{ display:inline-block; font-size:10px; color:#b5b5b5; line-height:10px; padding:2px 3px; border:1px solid #e0e0e0; margin-right:7px; border-radius:2px;}
.lp-xinxi{ padding:8px 15px 0;}
.lp-xinxi li{ width:100%; line-height:25px; font-size:14px; color:#010101;}/*20170207*/
.lp-xinxi li span{ float:left; margin-right:8px; color:#b5b5b5;}
.lp-xinxi li span i{ margin-right:2em;}
.lp-xinxi li span em{ margin-right:2em;}
.lp-xinxi li span i{ margin-right:0.5em;}
.lp-xinxi li p{ overflow:hidden; }/*20170207*/
.lp-xinxi li p a{ display:block; overflow:hidden; text-overflow:ellipsis; white-space:nowrap;}/*20170117*/
.lpxx-more{ padding:8px 15px 15px; line-height:20px;}
.lpxx-more a{ position:relative; display:inline-block; font-size:12px; color:#6c9cc6; padding-right:12px;}
.lpxx-more a:after{ position:absolute; content:''; width:10px; height:7px; top:5px; right:-2px; background: url(https://static.soufunimg.com/common_m/m_wechat/public/images/arr_down2.png) center no-repeat; background-size:100% auto;}
.lpxx-more a.arr-up:after{ transform:rotate(180deg);}

.hTitle{ margin: 0 15px; line-height: 54px; font-size:15px; color:#000000; border-bottom:1px solid #f8f8f8;}
.hTitle a{ display:block; position:relative; padding-right:10px;}
.hTitle a:after{ position:absolute; content:''; width:6px; height:8px; top:20px; right:0; background:url(https://static.soufunimg.com/common_m/m_wechat/public/images/arr_right.png) center no-repeat; background-size:100% auto; }
.hTitle a span{ font-size:12px;}
.hTitle a span i{ color:#df3031;}
.hxScroll{ position:relative; overflow:hidden; padding-bottom:20px;}
.hxScroll ul{ padding-left:15px; overflow:hidden;}
.hxScroll li{ width:144px; float:left; margin-right:20px;}
.hxScroll li > a{ display:block;}
.hxScroll li .hx-cont{ font-size:14px; color:#000000; line-height:24px; padding-top:10px; height:77px;}/*20170208*/
.hxScroll li .hx-cont p{ overflow:hidden; white-space:nowrap; text-overflow:inherit;font-size:12px;}/*20170209*/
.hxScroll li .hx-cont p.red{ color:#df3031; margin-top:5px;}/*20170209*/
.hxScroll li .hx-cont p span{ font-size:12px; color:#b5b5b5;}
.hxScroll li .star-s{ line-height:20px; margin-top:4px;}
.hxScroll li .star-s span{ float:right; margin-right:30px; font-size:12px; color:#000000;}
.hxScroll li .star-s i{ display:inline-block; width:8px; height:8px; margin-right:2px; background-image: url(https://static.soufunimg.com/common_m/m_wechat/public/images/icon_star.png); background-position:0 0; background-repeat:no-repeat; background-size:100% auto;}
.hxScroll li .star-s i.active{ background-position:0 -8px;}
.hxScroll li .star-s i.half{ background-position:0 -16px;}
.hxScroll li .hx-pic{ width:113px; height:113px; border:1px solid #f8f8f8; margin:10px 0;}
.hxScroll li .hx-pic img{ width:100%;}

.newsTrend{ padding:12px 15px 18px;}
.newsTrend > a{ display:block;}
.newsTrend .h4{ font-size:15px; color:#333333; line-height:20px;}
.newsTrend .xq-text{ padding-top:8px; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 3; overflow: hidden;}
.newsTrend .xq-text p{ font-size:14px; color:#888888; line-height:24px;}
.newsTrend .newsTap{ font-size:12px; color:#b5b5b5; padding-top:8px; line-height:15px;}

.x-comment-all{ margin:0 14px; padding-top:10px; padding-bottom:15px;}
.x-comment-all > dl{color:#83868f;overflow:hidden;line-height:24px;}
.x-comment-all dt{float:left;font-size:12px;color:#333333;}
.x-comment-all dd{float:left;font-size:12px;color:#565c67;margin-right:7px}
.x-comment-all dd span{margin-right:12px;}
.x-comment-all dd span:last-child{margin-right:0;}
.x-comment-all .ico-star{margin-top:7px;line-height:0;}
.ico-star i{ display: inline-block; width:12px; height:12px; margin-right:3px; background-image: url(https://static.soufunimg.com/common_m/m_wechat/public/images/icon_star.png); background-position:0 0; background-repeat:no-repeat; background-size:100% auto;}
.ico-star i.active{ background-position:0 -12px;}
.ico-star i.half{ background-position:0 -24px;}
.x-comment-all .num{line-height:24px;color:#888888;font-size:11px;}
.x-comment-all .num span{margin-right:6px;}
.x-comment-all .num span:last-child{margin-right:0;}

.x-comment-list{ margin:0 14px; border-bottom:1px solid #f8f8f8; overflow:hidden;}
.x-comment-list li{ margin-bottom:15px;}
.x-comment-list li a{ display:block;}
.x-comment-head{ height:25px; overflow: hidden;}
.x-comment-head dt{ float: left; margin-right: 8px; width: 25px; height: 25px; border-radius: 50%; overflow: hidden;}
.x-comment-head dt img{ width: 100%; height: 100%;}
.x-comment-head dd{ overflow: hidden; height:25px;}
.x-comment-head dd .h3{ font-size: 12px;color:#b5b5b5; line-height:1; padding-top:2px;}
.x-comment-head dd .stars{ height:12px;}
.x-comment-head dd .ico-star{ float:left; height:8px; margin-top:3px;}
.x-comment-head dd .ico-star i.active{ background-position:0 -8px;}
.x-comment-head dd .ico-star i.half{ background-position:0 -16px;}
.x-comment-head dd .time{ position:relative; float:right;font-size:10px; color:#b5b5b5; line-height:12px;}
.x-comment-head dd .time i{ position:absolute; display:block; width:22px; height:22px; bottom:0; right:0; background: url(https://static.soufunimg.com/common_m/m_wechat/public/images/icon_jing.png) center no-repeat; background-size:100% auto; z-index:9;}
.x-comment-head .ico-star i{ float:left; width:8px; height:8px;}
.x-comment-text{ line-height:24px; margin-top:10px; font-size: 14px; color:#333333; text-align:justify; overflow:hidden;}
.x-dp-btn{ margin:0 14px; height:50px; line-height:50px;}
.x-dp-btn a{ display:block; font-size:13px; color:#ff9596; text-align:center;}

.map-xq{ margin:0 14px;}
.map-xq li{ width:100%; height:50px; line-height:50px; font-size:14px; white-space:nowrap; text-overflow:ellipsis; overflow:hidden; border-bottom:1px solid #eeeeee;}
.map-xq li .arr-a{ display:block; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;}

.xqZygw {overflow:hidden;}
.xqZygw .list{ margin:0 14px; padding:7px 0;}
.xqZygw .list li{ width:100%; padding: 8px 0; overflow: hidden;}
.xqZygw .list .img{ position:relative; float: left; margin-right: 10px; width: 36px; height: 36px;}/*20170117*/
.xqZygw .list .img > a{display:inline;}
.xqZygw .list .img image{ width: 100%; height: 100%; border-radius: 50%;}
.xqZygw .list .img .icon-f{ position:absolute; bottom:1px; right:0; width:10px; height:10px; background:url(https://static.soufunimg.com/common_m/m_wechat/public/images/icon_f.png) no-repeat;background-size:10px;}
.xqZygw .list .txt{ float:left; font-size:12px; color:#b5b5b5; line-height:20px; vertical-align:middle; margin-top:-1px;max-width:70%;
}
.xqZygw .list .txt .h3{ color:#333333; line-height:15px;}
.xqZygw .list .txt p .red{ font-size:10px; color:#ff9596; line-height:12px; padding:2px 4px; border:1px solid #ffc2c3; border-radius:2px; margin-right:6px;}
.xqZygw .list .txt p.mrt3{ margin-top:3px;}
.xqZygw .kf-call{ width:32px; height:24px; padding-top:12px;}
.kf-call a { float: left; width: 30px; height: 22px; border: 1px solid #fd8787; border-radius: 2px; margin-right: 8px;}
.kf-call a i { display: block; width: 20px; height: 20px; margin: 2px auto; background: url(https://static.soufunimg.com/common_m/m_wechat/public/images/tel_icons2.png) no-repeat; background-size: 40px;}
.kf-call a.discall{ border: 1px solid #cccfd8;}
.kf-call a.discall i{background-position:0 -20px;}

.zyrk{ color: #fa682e !important; border: 1px solid #fa682e !important; white-space: nowrap;}
.zyrk em{ padding-left:2px; padding-right: 2px; padding-top: 1px; position: relative; background-color: #fa682e; color: #fff; margin-left:-3px; margin-right:3px !important;}
.stag span.ching{ background:#fff; border:1px solid #4fb6bf; color:#4fb6bf;}
.stag span.purple{ background:#fff; border:1px solid #c88bf5; color:#c88bf5;}
.stag span.pink-2{ background:#fff; border:1px solid #ffc2c3; color:#ff9596;}

/*底部弹框  电话*/
.bot-call{ position:fixed; bottom:0; left:0; width:100%; height:50px; background:#f7f7f7; z-index:99;}/*20170123*/
.bot-call .flor{ width:106px; line-height:50px; text-align:center; background-color:#df3031;}
.bot-call .flor a{ display:block; color:#fff;}
.bot-call .call-num { margin-left:20px; margin-right:106px;}
.bot-call .call-num a{ display:block; height:50px; font-size:12px; color:#888888;}
.bot-call .call-num a p{ position:relative; height:15px; padding-top:10px; padding-left:25px;} 
.bot-call .call-num a p:before{ position:absolute; content:''; width:20px; height:20px; bottom:0; left:0; background:url(https://static.soufunimg.com/common_m/m_wechat/public/images/icon_loufang.png) center no-repeat; background-size:100% auto;}
.bot-call .call-num span.tel1{ display:block; font-size:15px; line-height:20px; color:#000000; padding-top:2px;} 
.bot-call .call-num span.tel2{ display:block; font-size:15px; line-height:1; color:#333333; padding-top:4px;}
.bot-call .call-num a > div{ padding:3px 0; overflow:hidden;}
.bot-call .call-num span.kf-name{ display:inline-block; font-size:12px; color:#888888; padding-top:5px;}
.bot-call .call-num .kf{ width:32px; height:42px; border:1px solid #dedede; margin-right:5px;}

/*详情*/
.xqfocus{ position:relative; background:#fff;}
.xqfocus .xqImg{ position:relative;}
.xqImg .num{ position: absolute; right: 9px; bottom: 14px; padding: 2px 12px; background: rgba(0,0,0,.7); border-radius: 9px; color: #fff; font-size: 12px;line-height:12px; z-index:5;}
.xqTitle{ padding:0 15px; line-height:48px;}
.xqTitle p{ font-size:17px; color:#000;}
.xqTitle .stag{ padding-bottom:16px; line-height:16px;}
/*基本信息*/
.xqInfor{ padding:0 15px; background:#fff;}
.partBox{ padding:16px 0; border-bottom:1px solid #f8f8f8;}
.partBox .part{ float:left; width:33%; font-size:14px; line-height:24px; text-align:center; color:#888; border-right:1px solid #f8f8f8;}
.partBox .part:last-child{ border-right:0;}
.partBox .part .red_df{ font-weight:bold; color:#df3031;}
.xqInfor ul{ padding-top:6px; padding-bottom:6px;}
.xqInfor dl{ height:50px; font-size:14px; line-height:49px; border-top:1px solid #f8f8f8;}
.xqInfor dl em{ margin-right:2em;}
.xqInfor dl i{ margin-right:0.5em;}
.xqInfor dt{ float:left; display:block; width:75px; height:50px; color:#888;}
.xqInfor dd span{ color:#b5b5b5;}
.xqInfor dd span.cur{color:#333;}
.xqInfor dd a{ color:#6c96c6;}
/*标题行高40px*/
.title2{ line-height:50px; border-bottom:1px solid #f8f8f8;}
.title2 .h2{ font-size:15px; color:#000; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;}
.title2 .h2 span{ font-size:12px;}
.title2 .arr-rt{ display:block;}
.title2 .red_df{ color:#df3031;}
/*文字盒子*/
.txtBox2{ padding-top:9px; padding-bottom:9px; font-size:14px; color:#333; line-height:25px;}
.txtBox2 .price{ color:#df3031;}
.txtBox2 em{ margin-right:8px;}
.txtBox2 .c888{ color:#888;}
/*文字介绍*/
.intro{ padding:12px 0; font-size:14px; line-height:25px;}
/*详情点评列表*/
.comment-head{ padding:15px 0; overflow: hidden;}
.comment-head dt{ float:left; margin-right:6px; width:25px; height:25px; border-radius:50%; overflow: hidden;}
.comment-head dt img{ width:100%; height:100%;}
.comment-head dd{overflow: hidden;height:26px;}
.comment-head dd p{ font-size:11px;color:#b5b5b5;line-height:12px;}
.comment-head dd .h3{ padding-bottom:2px;}
.comment-head dd .kf-call a{ margin-right:0;}
.dpList .numBox{ font-size:11px; color:#333;}
.dpList .numBox span{ display:block; float:left; width:32%; text-align:right;}
.dpList .numBox span.first{ width:36%; text-align:left;}
.dpList .numBox span i{ color:#df3031;}
.dpList li{ position:relative;}
.dpList li .tuzh{ position:absolute; top:2px; right:44px; width:48px; height:48px; background:url(https://static.soufunimg.com/common_m/m_wechat/public/images/tz_yk.png) no-repeat; background-size:100%;}

/*免责声明*/
.mzsm{ padding:8px 15px; font-size:9px; color:#b5b5b5; line-height:12px; background:#f0f0f0;}

/*悬浮按钮*/
.fixedBtn{ padding-bottom:49px; width:100%;}
.fixedBtn a{ position:fixed; bottom:0; left:0; width:100%;display:block; font-size:16px; text-align:center; line-height:49px; color:#fff; background:#df3031;}

/*搜房帮*/
/*信息*/
.xqsper{ margin:0 15px; padding:16px 0; overflow:hidden; border-bottom: 1px solid #f8f8f8;}
.xqsper li{ width:33.1%; float:left; text-align:center; border-right: 1px solid #f8f8f8;}
.xqsper li.last{ border:none;}
.xqsper li a{ display:block; font-size:14px; color:#888888; line-height:24px;}
.xqsper li a span{ display:block; color:#dd3237; font-weight:bold;}
.fj-table{ margin:0 15px; padding:5px 0; border-bottom: 1px solid #f8f8f8;}
.fj-table li i{ margin-right:2em;}
.flextable li.w100{ width:100%;}
.flextable li p a{ font-size:11px; color:#b5b5b5;}

.xqTlist{ font-size:14px; color:#333333; padding:0 15px;}
.xqTlist li{ border-bottom:1px solid #f4f4f4; line-height:50px;}
.xqTlist a{ display:block; color:#83868f; white-space:nowrap; text-overflow:ellipsis; overflow:hidden;}/*20170117*/
.xqTlist a i{ margin-right:2em;}
.xqTlist span{ color:#3c3f46;}
.xqTlist span.blue{ color:#6c96c6;}

.fyms{ font-size:14px; color:#333333; line-height:24px; padding:12px 15px; text-align:justify;}
.cjjlBox{padding:16px 15px; line-height:24px;font-size:14px;color:#333;}
.cjjlBox p span.red{ color:#df3031;}
.cjjlBox p span.gray{ color:#888888;}

.pdX15{ padding:0 15px;}
.houseList li .txt .h2{ font-size:15px; color:#000000; height:36px; overflow:hidden; margin-bottom:4px; display: -webkit-box; -webkit-box-orient: vertical; -webkit-line-clamp: 2; overflow: hidden;}/*20170117*/
.houseList li .txt p.pdt3{ padding-top:2px;}
.houseList li .txt p.pdt3 .price{ font-size:15px;}
