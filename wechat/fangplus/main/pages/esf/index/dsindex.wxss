/* pages/esf/index/index.wxss */

/*列表红包样式*/
.houseList li div.yelbag{display:inline-block;line-height:12px;font-size:10px;color:#333333;border:1px solid #fd994e;white-space:nowrap;}
.houseList li div.yelbag .rb{position:relative;display:inline-block;padding:1px 5px 0;background-color:#fd994e; color:#fff;}
.houseList li div.yelbag .surp{display: inline-block;padding:1px 3px 0;height:12px;line-height:12px;background:#fff;}
.houseList li div.yelbag .surp .yel_fd{ color:#fd994e;}
/* 添加搜索列表样式*/
search{ position:relative; padding:8px;margin:0 auto; background:#efeff4; border-bottom:1px solid #d6d6db;z-index:2;}
.searbox{ position: relative; height:28px; font-size:14px; border:1px solid #e6e6ea; border-radius:4px; overflow: hidden;}
.searbox .ipt{ position: relative; margin-right:41px; background-color:#fff;}
.searbox .ipt input{padding: 0 4px; border: none; width: 90%; height: 28px;font-size: 14px;outline: none;-webkit-appearance: none;color:#0c0d0e;}
.searbox .ipt input::-webkit-input-placeholder{color:#888888;line-height:16px;}
.searbox .ipt .off{ position: absolute; top: 50%; margin-top: -12px; right:6px; display: block; width: 24px; height: 24px; background:#fff url(http://static.soufunimg.com/common_m/m_wechat/public/images/search_off.png) center no-repeat; background-size: 14px auto;}
.searbox .btn{ position:absolute; right:0;top:0;width:41px; height:28px; line-height:26px; text-align:center;border-left:1px solid #e5e7ed;border-radius:0 3px 3px 0;background:#f3f5f7;}
.searbox .btn i{ margin-top:6px; display:inline-block; width:17px; height:17px; background:url(http://static.soufunimg.com/common_m/m_wechat/public/images/icon_search.png) 0 0 no-repeat; background-size:15px auto;}/*20170103 end*/


.scTab li a{ display:block; margin-top:12px; height:25px; line-height:25px; text-align:center; color:#353535; border-left:1px solid #f0f0f0; padding:0 3px;}/*20170118*/
.scTab li span{position:relative;display:inline-block; max-width:100%; line-height:25px;overflow:hidden;text-overflow:ellipsis;white-space:nowrap; padding-right:10px; -webkit-box-sizing: border-box; -moz-box-sizing: border-box; -ms-box-sizing: border-box; box-sizing: border-box;}/*20170118*/