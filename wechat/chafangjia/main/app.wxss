@charset "utf-8";
/* CSS Document */
html, body, page{margin:0; padding:0; height:100%;}
form, fieldset, div, view, p, ul, ol, li, dl, dt, dd{ -webkit-text-size-adjust:none;margin:0; padding:0;font-family:Arial, "Microsoft Yahei"; list-style-type:none; display: block;}
i, em, span,text{margin:0; padding:0;font-style:normal;}
form, img,image, table, td { border:0;margin:0; padding:0;}
input, textarea, img, image {margin:0; padding:0; vertical-align:middle; font-family:Arial, "Microsoft Yahei";-webkit-tap-highlight-color: rgba(0,0,0,0); outline:none;}
a {margin:0; padding:0;color:#000;text-decoration:none;}
swiper{height: 280px}

